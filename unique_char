#include<iostream>
#include<string>
#include<algorithm>
using namespace std;


int main()
{
	string s1;
	string s2;
	string s3;
	cout<<"enter a string s1:";
	cin>>s1;
	cout<<endl<<"enter a string s1:";
	cin>>s2;
	sort(s1.begin(),s1.end());
	sort(s2.begin(),s2.end());

	for(int i=0;i<s1.length();i++)
	{
		if(s2.at(i)!=s1.at(i))
		{
		cout<<s2.at(i);
		break;
		}
		else if(i==s1.length()-1)
		{
	     cout<<endl<<"New char found in s2:";
		 cout<< s2.at(i+1);
	
		}
	
	}


		cin>>s2;


	return 0;


}
